  file = {D\:\\literature\\LiteratureStore\\Nature Communications\\2016\\ren_et_al_2016_direct_evidence_of_two_interatomic_relaxation_mechanisms_in_argon_dimers.pdf;D\:\\literature\\databases\\PHzot2020_Stimpy\\storage\\FQ9FLVKU\\ncomms11093.html},
}


@article{ren2018ExperimentalEvidenceUltrafast,
  title = {{Experimental Evidence for Ultrafast Intermolecular Relaxation Processes in Hydrated Biomolecules}},
  author = {Ren, Xueguang and Wang, Enliang and Skitnevskaya, Anna D. and Trofimov, Alexander B. and Gokhberg, Kirill and Dorn, Alexander},
  year = {2018},
