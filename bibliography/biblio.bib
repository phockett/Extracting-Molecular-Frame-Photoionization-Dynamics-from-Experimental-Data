@article{karamatskos2019MolecularMovieUltrafast,
  title = {{Molecular Movie of Ultrafast Coherent Rotational Dynamics of {{OCS}}}},
  author = {Karamatskos, Evangelos T. and Raabe, Sebastian and Mullins, Terry and Trabattoni, Andrea and Stammer, Philipp and Goldsztejn, Gildas and Johansen, Rasmus R. and D{\l}ugo{\l}ecki, Karol and Stapelfeldt, Henrik and Vrakking, Marc J. J. and Trippel, Sebastian and Rouz{\'e}e, Arnaud and K{\"u}pper, Jochen},
  year = {2019},
