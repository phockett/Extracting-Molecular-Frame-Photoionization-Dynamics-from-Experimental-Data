  author+an = {1=lead},
  copyright = {All rights reserved},
  langid = {english},
  keywords = {Physics - Atomic Physics,Physics - Chemical Physics,Publication,Quantum Physics},
  file = {D\:\\literature\\LiteratureStore\\arXiv2012.04561 [physics, physicsquant-ph]\\2020\\Gregory_et_al_2020_Molecular_Frame_Photoelectron_Angular_Distributions_in_Polyatomic_Molecules.pdf;D\:\\literature\\LiteratureStore\\Journal of Physics B Atomic, Molecular and Optical Physics\\2021\\Gregory_et_al_2021_Towards_molecular_frame_photoelectron_angular_distributions_in_polyatomic.pdf;D\:\\literature\\databases\\PHzot2020_Stimpy\\storage\\GAHUH5TL\\2012.html}
