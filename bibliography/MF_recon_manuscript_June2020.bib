@article{Akoury2007,
  title = {The Simplest Double Slit: Interference and Entanglement in Double Photoionization of {{H2}}.},
  author = {Akoury, D and Kreidi, K and Jahnke, T and Weber, Th and Staudte, A and Sch{\"o}ffler, M and Neumann, N and Titze, J and Schmidt, L Ph H and Czasch, A and Jagutzki, O and Costa Fraga, R A and Grisenti, R E and D{\'i}ez Mui{\~n}o, R and Cherepkov, N A and Semenov, S K and Ranitovic, P and Cocke, C L and Osipov, T and Adaniya, H and Thompson, J C and Prior, M H and Belkacem, A and Landers, A L and {Schmidt-B{\"o}cking}, H and D{\"o}rner, R},
